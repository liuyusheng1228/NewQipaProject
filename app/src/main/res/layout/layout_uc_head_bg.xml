<?xml version="1.0" encoding="utf-8"?>
<com.qipa.jetpackmvvm.widget.DisInterceptNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_collapseMode="parallax"
    android:clipChildren="false"
    android:clipToPadding="false"
    app:layout_collapseParallaxMultiplier="0.8">

    <RelativeLayout
        android:id="@+id/user_head_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <ImageView
            android:id="@+id/gm_up_um_bg"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_250"
            android:src="@mipmap/bg_task_center_header"
            android:scaleType="fitXY"
            android:tag="overScroll" />

    </RelativeLayout>

</com.qipa.jetpackmvvm.widget.DisInterceptNestedScrollView>